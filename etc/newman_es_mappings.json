  "settings" : {
    "index" : {
      "mapping" : {
        "allow_type_wrapper" : "true"
      },
      "number_of_shards" : "1",
      "number_of_replicas" : "0",

      "analysis" : {
        "analyzer" : {
          "account_number_analyzer" : {
            "tokenizer": "account_number_tokenizer",
            "filter":   [
              "account_number_normalization_filter"
            ]
          },
          "domain_analyzer" : {
            "char_filter" : [ "domain_pattern" ],
            "tokenizer" : "standard"
          },
          "trigram_analyzer": {
            "type":      "custom",
            "tokenizer": "standard",
            "filter":   [
              "lowercase",
              "trigram_filter"
            ]
          },
          "trigram_d_analyzer": {
            "type": "custom",
            "tokenizer": "trigram_d_tokenizer",
            "filter": [
              "lowercase"
            ]
          }
        },
        "char_filter" : {
          "domain_pattern" : {
            "type" : "pattern_replace",
            "pattern" : ".*@",
            "replacement" : ""
          }
        },
        "filter": {
          "trigram_filter": {
            "type":     "ngram",
            "min_gram": 3,
            "max_gram": 3
          },
          "account_number_normalization_filter": {
            "type" : "pattern_replace",
            "pattern" : "[ -]",
            "replacement" : ""
          }
        },
        "tokenizer": {
          "trigram_d_tokenizer": {
            "type": "nGram",
            "min_gram": 3,
            "max_gram": 3,
            "token_chars": [ "digit" ]
          },
          "account_number_tokenizer" : {
            "type" : "pattern",
            "pattern" : "([\\d]{4}[ -]?[\\d]{4}[ -]?[\\d]{4}[ -]?[\\d]{4})",
            "group" : 0
          }
        }
      }
    }
  }
